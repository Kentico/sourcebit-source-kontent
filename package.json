{
  "name": "sourcebit-source-kontent",
  "version": "0.1.0",
  "description": "Stackbit Source Kontent Plugin",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "npm run format",
    "format": "prettier --write \"./**/*.{js,jsx,md,html}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makma/sourcebit-source-kontent.git"
  },
  "keywords": [
    "headless",
    "headless-cms",
    "jamstack",
    "ssg",
    "kontent"
  ],
  "author": "Martin Makarsky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makma/sourcebit-source-kontent/issues"
  },
  "homepage": "https://github.com/makma/sourcebit-source-kontent#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "husky": "^4.0.0",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "retry-axios": "^2.1.2",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,md,html}": [
      "npm run format",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2"
  }
}
